{
  "name": "vscode-ember",
  "description": "Visual Studio Code plugin for the Ember Language Server",
  "author": "Thomas Sauer",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "t-sauer",
  "engines": {
    "vscode": "^1.8.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:handlebars",
    "onLanguage:javascript"
  ],
  "main": "./out/src/extension",
  "contributes": {
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "update-vscode": "node ./node_modules/vscode/bin/install",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.33",
    "@types/node": "^6.0.52",
    "typescript": "^2.1.4",
    "vscode": "^1.0.3"
  },
  "dependencies": {
    "vscode-languageclient": "^2.6.3"
  }
}
